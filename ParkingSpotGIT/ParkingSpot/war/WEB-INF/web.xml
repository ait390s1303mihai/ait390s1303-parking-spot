<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ParkingSpot</servlet-name>
		<servlet-class>parkingspot.ParkingSpotServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ParkingSpot</servlet-name>
		<url-pattern>/parkingspot</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddCampusServlet</servlet-name>
		<servlet-class>parkingspot.AddCampusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddCampusServlet</servlet-name>
		<url-pattern>/admin/addCampusCommand</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteCampusServlet</servlet-name>
		<servlet-class>parkingspot.DeleteCampusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteCampusServlet</servlet-name>
		<url-pattern>/admin/deleteCampusCommand</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateCampusServlet</servlet-name>
		<servlet-class>parkingspot.UpdateCampusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateCampusServlet</servlet-name>
		<url-pattern>/admin/updateCampusCommand</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>allCampus.jsp</welcome-file>
	</welcome-file-list>
	</web-app>
